CREATE TABLE IF NOT EXISTS ${hiveconf:database}.${hiveconf:table_name}  (, insert_tst  TIMESTAMP COMMENT 'Timestamp for creation of record.', source_system  INTEGER COMMENT 'Name of source system.')  COMMENT 'Die Rows in Table lv repraesentieren einen oder mehrere Vertragsstaende; temporal identifiziert durch das Field lvId. Die Vertragsstaende sind von 1 beginnend durchnummeriert, wobei es auch Luecken geben darf. Welche Vertragsstaende eine Row repraesentie' PARTITIONED BY (tenant_name  STRING COMMENT 'Name of the tenant.', process_id  INTEGER COMMENT 'Unique id for ETL-Processes.')  STORED AS parquet;

