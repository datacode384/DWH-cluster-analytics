CREATE TABLE IF NOT EXISTS  ${hiveconf:database}.${hiveconf:table_name}  (steuwertekum_sid  STRING COMMENT 'Surrgate Key', steuwertekum_id  STRING COMMENT 'Foreign Key Key table', lvid  STRING COMMENT 'id of Lv (Das Field lvId identifiziert denVertrag, zu dem diese Row gehoert.)', bearbnw_sid  STRING COMMENT 'Foreign Key', lv_sid  STRING COMMENT 'Foreign Key', steuschicht_sid  STRING COMMENT 'Foreign Key', vt_sid  STRING COMMENT 'Foreign Key', bearbid  INTEGER COMMENT 'the historical version number (logId)', bearbnw_id  STRING COMMENT 'Foreign Key', lv_id  STRING COMMENT 'Foreign Key', steuschicht_id  STRING COMMENT 'Foreign Key', vt_id  STRING COMMENT 'Foreign Key', vtid  SMALLINT COMMENT 'id of Vt (generically derived from Vt)', bearbidabg  INTEGER COMMENT 'the historical version that invalidates this version (nextLogId)', schichtid  SMALLINT COMMENT 'id of SteuSchicht (Zur Identifizierung der Steuerschicht)', zuz  DECIMAL(2,2) COMMENT 'Summe aller Zuzahlungen', btgsteu  DECIMAL(2,2) COMMENT 'Summe der istBeitraege', abfluss  DECIMAL(2,2) COMMENT 'Abfluesse aus Vertragsteil durch vertragsinterne Umbuchungen', zufluss  DECIMAL(2,2) COMMENT 'Zufluss zum Vertragsteil aus vertragsinternen Umbuchungen oder anrechenbaren Werten', foeinzausz  DECIMAL(2,2) COMMENT 'Summe der Einzahlungen in Fonds', ziertr  DECIMAL(2,2) COMMENT 'Summe der Zinsertraege am Baustein', partkey  SMALLINT COMMENT 'the partition key', zulage  DECIMAL(2,2) COMMENT 'Summe der Zulagen', foeinzauszstbel  DECIMAL(2,2) COMMENT 'Fondsein- und auszahlungen zu steuerbelasteten Fonds, die als Publikumsfonds im Sinne des ', foertrstbelinv  DECIMAL(2,2) COMMENT 'Netto-Ertrag, der aus der Anlage in steuerbelasteten Publikumsfonds (gem. Investment-Steue', kosteninvdelta  DECIMAL(2,2) COMMENT 'Kosten oder Kostenkorrekturen im Sinne des Investmentsteuergesetzes', kosteninvsteuer  DECIMAL(2,2) COMMENT 'Summe der Kosten im Sinne des Investmentsteuergesetzes im steuerl. Fortschreibungsinterval', diff_hash  STRING COMMENT 'Hash for comparing records for inserts.', record_hash  STRING COMMENT 'Hash for all non technical attributes.', insert_tst  TIMESTAMP COMMENT 'Timestamp for creation of record.', source_system  INTEGER COMMENT 'Name of source system.', primary key(bearbidabg, lvid, schichtid, vtid) disable novalidate ) COMMENT 'Steuerliche Werte werden pro Steuerschicht berechnet. Dazu ist es notwendig gewisse Werte im Laufe der Vertragsentwicklung zu dokumentieren: Beispielsweise die Summe an Istbeitraegen oder Zu- bzw. Abfluesse vom Baustein, die dieser Steuerschicht zugeordn' PARTITIONED BY (tenant_name  STRING COMMENT 'Name of the tenant.', process_id  INTEGER COMMENT 'Unique id for ETL-Processes.')  STORED AS parquet;