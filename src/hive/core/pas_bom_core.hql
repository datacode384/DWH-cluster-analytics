CREATE TABLE IF NOT EXISTS  core.pas_bom (bom_sid  STRING COMMENT 'Surrgate Key', bom_id  STRING COMMENT 'Foreign Key', entity  STRING, attribut  STRING, typ  STRING, qualification  STRING, customer  STRING, diff_hash  STRING COMMENT 'Hash for comparing records for inserts.', record_hash  STRING COMMENT 'Hash for all non technical attributes.', insert_tst  TIMESTAMP COMMENT 'Timestamp for creation of record.', source_system  INTEGER COMMENT 'Name of source system.', primary key(entity, attribut) disable novalidate ) PARTITIONED BY (process_id  INTEGER COMMENT 'Unique id for ETL-Processes.')  STORED AS parquet TBLPROPERTIES ('PARQUET.COMPRESS'='SNAPPY');