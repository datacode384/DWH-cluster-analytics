CREATE TABLE IF NOT EXISTS  core.par_bnka (diff_hash  STRING COMMENT 'Hash for comparing records for inserts.', gueltig_ab  TIMESTAMP COMMENT '', insert_tst  TIMESTAMP COMMENT 'Timestamp for creation of record.', par_bnka_sid  STRING COMMENT 'Surrgate Key', record_hash  STRING COMMENT 'Hash for all non technical attributes.', source_system  INTEGER COMMENT 'Name of source system.', z_cre_dida  INTEGER COMMENT 'Timestamp initial import DiDa', z_ini_tims  INTEGER COMMENT 'Timestamp initial transfer', z_logsys_src  INTEGER COMMENT 'Source', z_upd_dida  INTEGER COMMENT 'Timestamp update import DiDa', z_upd_tims  INTEGER COMMENT 'Timestamp last updated transfer', par_bnka_id  STRING COMMENT 'Foreign Key', mandt  STRING COMMENT 'Mandant', banks  STRING COMMENT 'Länderschlüssel der Bank', bankl  STRING COMMENT 'Bankschlüssel', erdat  STRING COMMENT 'Datum, an dem der Satz hinzugefügt wurde', ernam  STRING COMMENT 'Name des Sachbearbeiters, der das Objekt hinzugefügt hat', banka  STRING COMMENT 'Name des Geldinstituts', provz  STRING COMMENT 'Region (Bundesstaat, Bundesland, Provinz, Grafschaft)', stras  STRING COMMENT 'Straße und Hausnummer', ort01  STRING COMMENT 'Ort', swift  STRING COMMENT 'SWIFT/BIC  für internationale Zahlungen', bgrup  STRING COMMENT 'Bankengruppe (Bankennetz)', xpgro  STRING COMMENT 'Postbank-Girokonto', loevm  STRING COMMENT 'Löschkennzeichen', bnklz  STRING COMMENT 'Bankleitzahl', tpa_mandant STRING, primary key(mandt, banks, bankl) disable novalidate ) PARTITIONED BY (process_id  INTEGER COMMENT 'Unique id for ETL-Processes.')  STORED AS parquet TBLPROPERTIES ('PARQUET.COMPRESS'='SNAPPY');
