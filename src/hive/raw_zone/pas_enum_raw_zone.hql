CREATE TABLE IF NOT EXISTS  raw_zone.pas_enum (enum  STRING, literal  STRING, value  STRING, shorttext  STRING, longtext  STRING, customer  STRING, insert_tst  TIMESTAMP COMMENT 'Timestamp for creation of record.', source_system  INTEGER COMMENT 'Name of source system.')  PARTITIONED BY (process_id  INTEGER COMMENT 'Unique id for ETL-Processes.')  STORED AS parquet TBLPROPERTIES ('PARQUET.COMPRESS'='SNAPPY');