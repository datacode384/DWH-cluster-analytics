CREATE TABLE IF NOT EXISTS  raw_zone.pas_bom (entity  STRING, attribut  STRING, typ  STRING, qualification  STRING, customer  STRING, insert_tst  TIMESTAMP COMMENT 'Timestamp for creation of record.', source_system  INTEGER COMMENT 'Name of source system.')  PARTITIONED BY (process_id  INTEGER COMMENT 'Unique id for ETL-Processes.')  STORED AS parquet TBLPROPERTIES ('PARQUET.COMPRESS'='SNAPPY');